{
  "name": "colada-plugin",
  "version": "0.1.0",
  "description": "A plugin to support Pinia",
  "author": {
    "name": "Jonathan Chen, Parker Steinberg, Vaughn Sulit, Dan Steinbrook"
  },
  "type": "module",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "browser": "./dist/colada-plugin.esm.js",
      "import": "./dist/colada-plugin.esm.js",
      "module": "./dist/colada-plugin.esm.js"
    },
    "./package.json": "./package.json"
  },
  "sideEffects": false,
  "scripts": {
    "prepublish": "npm run dev",
    "dev": "npm run dev:vite",
    "dev:vite": "vite build --watch",
    "dev:rollup": "rimraf dist && rollup --config rollup.config-new.mjs --watch",
    "dev:rollup:old": "rimraf dist && rollup --config rollup.config.mjs --watch",
    "ts": "tsc --watch -d",
    "test": "vitest",
    "coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@rollup/plugin-commonjs": "^22.0.2",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-replace": "^4.0.0",
    "@types/node": "^18.7.14",
    "@vue/devtools-api": "^6.2.1",
    "lodash": "^4.17.21",
    "pascalcase": "^2.0.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.79.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.33.0",
    "rollup-plugin-vue": "^6.0.0",
    "typescript": "^4.8.2"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.185",
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "@typescript-eslint/parser": "^5.37.0",
    "eslint": "^8.23.1",
    "eslint-plugin-vue": "^9.5.1",
    "jsdom": "^20.0.0",
    "vitest": "^0.23.4"
  }
}
